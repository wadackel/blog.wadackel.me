{
  "private": true,
  "type": "module",
  "scripts": {
    "build": "astro build",
    "dev": "astro dev --host",
    "format": "run-s format:*",
    "format:lint": "npm run lint -- --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml}\"",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "astro preview --host",
    "scaffold": "scaffdog generate",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,ts,tsx,css,md,yml,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 2 versions"
  ],
  "prettier": {
    "arrowParens": "always",
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-astro",
      "prettier-plugin-scaffdog"
    ],
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^8.1.0",
    "@astrojs/partytown": "^2.0.3",
    "@astrojs/preact": "^3.1.0",
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@astrojs/mdx": "^2.0.4",
    "@astrojs/rss": "^4.0.2",
    "@astrojs/sitemap": "^3.0.4",
    "@astrojs/tailwind": "^5.1.0",
    "@cloudflare/workers-types": "^4.20231218.0",
    "@eslint/js": "^8.57.0",
    "@fec/remark-a11y-emoji": "^4.0.2",
    "@fontsource/caveat": "^5.0.16",
    "@resvg/resvg-js": "^2.6.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "astro": "^4.1.2",
    "autoprefixer": "^10.4.16",
    "dayjs": "^1.11.10",
    "eslint": "^9.25.0",
    "eslint-plugin-import": "^2.29.1",
    "front-matter": "^4.0.2",
    "globals": "^16.0.0",
    "globby": "^14.0.0",
    "graphemesplit": "^2.4.4",
    "hast-util-to-html": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "mdast-util-to-string": "^4.0.0",
    "mrmime": "^2.0.0",
    "npm-run-all2": "^6.0.0",
    "parse-numeric-range": "^1.3.0",
    "postcss-import": "^16.0.0",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.2.2",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-scaffdog": "3.0.0",
    "prismjs": "^1.29.0",
    "quicklink": "^2.3.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-emoji": "^4.0.1",
    "remark-gfm": "^4.0.0",
    "remark-rehype": "^11.1.0",
    "remark-slug": "^7.0.1",
    "satori": "^0.10.11",
    "scaffdog": "3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0"
  },
  "packageManager": "pnpm@8.15.2"
}
